{
  "build": {
    "env": {
      "PRISMA_GENERATE_DATAPROXY": "true",
      "SKIP_ENV_VALIDATION": "1"
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/src/app/api/:path*"
    }
  ]
}